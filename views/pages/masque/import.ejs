  /*
  * Fonction pour afficher la liste deroulante des teleconseilaire
  * */

  function loadTC(data){
    if(data == "u") data = "";
    $.ajax({
      type: "GET",
      url: "/getLsTC?sql="+data,

      success: function(msg){

        var html = "<option value='u'>TOUTES LES TC</option>";
        var data = JSON.parse(msg);

        for (var i = 0 ; i<data.length ; i++){
          html += "<option value='"+data[i].matricule+"'>"+data[i].pseudo+" - "+data[i].matricule+"</option>";
        }
        $("#tc_select").html(html);
      },
      error: function (error) {

      }
    });
  }

  /*
  Liste des chef d'equipe
  */

  function loadCE(data){
    if(data == "u") data = "";
    $.ajax({
      type: "GET",
      url: "/getLsCE?sql="+data,

      success: function(msg){

        var html = "<option value='u'>TOUTES LES EQUIPES</option>";
        var data = JSON.parse(msg);

        for (var i = 0 ; i<data.length ; i++){
          html += "<option value='"+data[i].matricule+"'>"+data[i].pseudo+"</option>";
        }
        $("#ce_select").html(html);
      },
      error: function (error) {

      }
    });
  }

